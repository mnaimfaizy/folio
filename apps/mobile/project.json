{
  "name": "mobile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mobile",
  "projectType": "application",
  "tags": ["scope:mobile", "type:app"],
  "targets": {
    "start": {
      "executor": "@nx/expo:start",
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "serve": {
      "executor": "@nx/expo:serve",
      "dependsOn": ["sync-deps"],
      "options": {
        "port": 4200
      }
    },
    "run-ios": {
      "executor": "@nx/expo:run",
      "dependsOn": ["sync-deps"],
      "options": {
        "platform": "ios"
      }
    },
    "run-android": {
      "executor": "@nx/expo:run",
      "dependsOn": ["sync-deps"],
      "options": {
        "platform": "android"
      }
    },
    "doctor": {
      "executor": "nx:run-commands",
      "dependsOn": ["sync-deps"],
      "options": {
        "cwd": "apps/mobile",
        "command": "../../node_modules/.bin/expo-doctor"
      }
    },
    "build": {
      "executor": "@nx/expo:build",
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "submit": {
      "executor": "@nx/expo:submit",
      "options": {}
    },
    "build-list": {
      "executor": "@nx/expo:build-list",
      "options": {}
    },
    "sync-deps": {
      "executor": "@nx/expo:sync-deps",
      "options": {
        "exclude": ["@expo/metro-config"],
        "include": ["react-native-screens"]
      }
    },
    "prebuild": {
      "executor": "@nx/expo:prebuild",
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "install": {
      "executor": "@nx/expo:install",
      "options": {}
    },
    "update": {
      "executor": "@nx/expo:update",
      "options": {}
    },
    "export": {
      "executor": "@nx/expo:export",
      "dependsOn": ["sync-deps"],
      "outputs": ["{options.outputDir}"],
      "options": {
        "platform": "all",
        "outputDir": "apps/mobile/dist"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/mobile"],
      "options": {
        "jestConfig": "apps/mobile/jest.config.cjs",
        "passWithNoTests": true
      }
    }
  }
}
